{
  "name": "Louder",
  "short_name": "louder",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Helps boost tab volume",
  "homepage_url": "https://githun.com/thewhodidthis/chrome-louder",
  "minimum_chrome_version": "61",
  "icons": {
    "16": "assets/icon.png",
    "32": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon":  "assets/icon.png"
  },
  "permissions": [
    "tabs",
    "tabCapture",
    "<all_urls>"
  ]
}
